{
  "expo": {
    "name": "ZT Compass",
    "slug": "zt-compass",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#0f172a"
    },
    "ios": {
      "bundleIdentifier": "ch.alpennova.ztcompass",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "ch.alpennova.ztcompass",
      "adaptiveIcon": {
        "backgroundColor": "#0f172a",
        "foregroundImage": "./assets/adaptive-icon.png"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3673e315-744c-46eb-9f26-b62db198343f"
      }
    },
    "owner": "kavinraghu"
  }
}